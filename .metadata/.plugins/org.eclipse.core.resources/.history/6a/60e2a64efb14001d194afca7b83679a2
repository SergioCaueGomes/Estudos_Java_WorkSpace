package poo.model;

import java.util.List;

public class Cliente {
	
	public Integer codigo;
	public String  nome;
	public String  cpf;
	
	private List<Endereco> enderecos;

	
	public List<Endereco> getEnderecos() {
		if(enderecos == null) {
			enderecos = new ArrayList<Endereco>
		}
		
		return enderecos;
	}
	
	
	
}
